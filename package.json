{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "example:build": "parcel build example/index.html",
    "example:nocache": "parcel --no-cache example/index.html",
    "example": "parcel example/index.html",
    "start": "tsdx watch --tsconfig tsconfig.build.json",
    "build": "tsdx build --tsconfig tsconfig.build.json",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "prettier": "prettier --write --loglevel warn --ignore-path .eslintignore .",
    "prettier:check": "prettier --check --ignore-path .eslintignore .",
    "lint": "eslint --fix .",
    "lint:fix": "npm run lint && npm run prettier",
    "prepare": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cors-proxy": "node example/cors-proxy.js"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "lf"
  },
  "name": "web-reader",
  "author": "kristojorg",
  "module": "dist/web-reader.esm.js",
  "size-limit": [
    {
      "path": "dist/web-reader.cjs.production.min.js",
      "limit": "650 KB"
    },
    {
      "path": "dist/web-reader.esm.js",
      "limit": "650 KB"
    }
  ],
  "staticFiles": {
    "staticPath": "example/static"
  },
  "example-app": "www/index.html",
  "targets": {
    "example-app": {
      "engines": {
        "browsers": "> 2%"
      },
      "scopeHoist": false,
      "sourceMap": true
    },
    "main": false,
    "module": false
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@emotion/jest": "^11.3.0",
    "@parcel/transformer-typescript-tsc": "^2.0.0-beta.3.1",
    "@size-limit/preset-small-lib": "^4.9.1",
    "@storybook/addon-actions": "^6.3.1",
    "@storybook/addon-essentials": "^6.3.1",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.1",
    "@storybook/addon-storyshots": "^6.3.1",
    "@storybook/addons": "^6.3.1",
    "@storybook/react": "^6.3.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.0",
    "@types/react-pdf": "^5.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-jest": "^27.0.5",
    "babel-loader": "^8.2.2",
    "esbuild-jest": "^0.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.9.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "^4.17.1",
    "husky": "^4.3.6",
    "jest": "^27.0.5",
    "jest-watch-typeahead": "^0.5.0",
    "parcel": "^2.0.0-beta.3.1",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "parcel-reporter-static-files-copy": "^1.3.0",
    "prettier": "2.2.1",
    "r2-shared-js": "^1.0.49",
    "react": "^17.0.1",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "request": "^2.88.2",
    "size-limit": "^4.9.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.6",
    "@d-i-t-a/reader": "^2.0.0-alpha.2",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@nypl/design-system-react-components": "^0.21.2",
    "framer-motion": "^4.1.6",
    "node-fetch": "^2.6.1",
    "react-pdf": "^5.3.2",
    "workbox-core": "^6.1.5",
    "workbox-routing": "^6.1.5",
    "workbox-window": "^6.1.5"
  }
}
